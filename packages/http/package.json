  ],
  "author": "David Zheng",
  "homepage": "https://github.com/feutopia/feutopia-monorepo/tree/main/packages/http#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feutopia/feutopia-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/feutopia/feutopia-monorepo/issues"
