{
  "name": "@feutopia/react-ui",
  "private": false,
  "version": "0.0.1",
  "description": "React UI Components Library",
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/lib/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run test --run && pnpm run build:vite",
    "test": "vitest",
    "build:vite": "tsc -b ./tsconfig.json && vite build"
  },
  "author": "David Zheng",
  "homepage": "https://github.com/feutopia/feutopia-monorepo/tree/main/packages/react-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feutopia/feutopia-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/feutopia/feutopia-monorepo/issues"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "clsx": "^2.1.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "glob": "^11.0.0",
    "globals": "^15.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
