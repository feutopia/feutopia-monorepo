{
  "name": "@feutopia/react-ui",
  "private": false,
  "version": "0.0.2",
  "description": "React UI Components Library",
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/lib/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/main.d.ts",
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "pnpm run typecheck && pnpm run test --run && pnpm run build:vite",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "build:vite": "tsc -p ./tsconfig.json && vite build"
  },
  "author": "David Zheng",
  "homepage": "https://github.com/feutopia/feutopia-monorepo/tree/main/packages/react-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feutopia/feutopia-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/feutopia/feutopia-monorepo/issues"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "clsx": "^2.1.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "glob": "^11.0.0",
    "globals": "^15.11.0",
    "happy-dom": "^15.11.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.81.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "5.4.11",
    "vite-plugin-dts": "4.3.0",
    "vite-plugin-lib-inject-css": "2.1.1",
    "vitest": "^2.1.5"
  }
}
