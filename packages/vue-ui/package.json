{
  "name": "@feutopia/vue-ui",
  "private": false,
  "version": "0.0.24",
  "description": "Vue3 UI Components Library",
  "keywords": [
    "vue3",
    "ui",
    "components"
  ],
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/lib/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/main.d.ts",
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "pnpm run typecheck && pnpm run test --run && pnpm run build:vite",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "build:vite": "vue-tsc -p ./tsconfig.json && vite build"
  },
  "author": "David Zheng",
  "homepage": "https://github.com/feutopia/feutopia-monorepo/tree/main/packages/vue-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feutopia/feutopia-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/feutopia/feutopia-monorepo/issues"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@feutopia/vue-hooks": "workspace:^0.0.12",
    "echarts": "^5.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.0",
    "@vue/test-utils": "^2.4.6",
    "glob": "^11.0.0",
    "happy-dom": "^15.11.6",
    "sass": "^1.81.0",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vite-plugin-dts": "4.3.0",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vitest": "^2.1.5",
    "vue": "3.5.12",
    "vue-tsc": "^2.1.8"
  },
  "license": "MIT"
}
