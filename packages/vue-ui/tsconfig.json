  "extends": "../../tsconfig.json",
  "include": ["lib/**/*.ts", "lib/**/*.tsx", "lib/**/*.vue", "lib/**/*.d.ts"],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
